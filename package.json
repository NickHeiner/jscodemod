{
  "name": "jscodemod",
  "version": "1.0.0",
  "description": "JS codemod runner",
  "main": "index.js",
  "repository": "git@github.com:NickHeiner/jscodemod.git",
  "author": "Nick Heiner <nth23@cornell.edu>",
  "license": "MIT",
  "bin": {
    "jscodemod": "./build/bin.js"
  },
  "scripts": {
    "jscodemod": "./build/bin.js",
    "build": "rm -rf build && tsc && chmod a+x ./build/bin.js",
    "demo": "yarn build && yarn jscodemod --codemod test/fixtures/arrow-function-inline-return/codemod/index.ts --tsconfig test/fixtures/arrow-function-inline-return/codemod/tsconfig.json test/fixtures/arrow-function-inline-return/source",
    "demo:react:prefix": "yarn build && yarn jscodemod --codemod test/fixtures/prepend-string/codemod.js 'test/fixtures/react/packages/**/*.js'",
    "test": "yarn lint",
    "lint": "eslint test src/**/*.ts --max-warnings 0 --cache",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@babel/plugin-syntax-pipeline-operator": "7.10.4",
    "@babel/plugin-transform-typescript": "7.11.0",
    "@types/lodash": "4.14.161",
    "@types/progress": "2.0.3",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "babel": "6.23.0",
    "eslint": "7.9.0",
    "eslint-config-nth": "2.0.1",
    "eslint-plugin-jest": "24.0.1",
    "jest": "26.4.2",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "find-up": "5.0.0",
    "globby": "11.0.1",
    "lodash": "4.17.20",
    "loud-rejection": "2.2.0",
    "nth-log": "1.1.0",
    "piscina": "1.6.3",
    "progress": "2.0.3",
    "resolve-bin": "0.4.0",
    "tempy": "0.7.0"
  }
}
